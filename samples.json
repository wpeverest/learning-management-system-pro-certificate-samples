    "html": "<!-- wp:masteriyo/certificate {\"backgroundImageURL\":\"{{masteriyo_a4_portrait_background_image_url}}\",\"backgroundImageID\":{{masteriyo_a4_portrait_background_image_id}},\"containerWidth\":{\"value\":80,\"unit\":\"%\"},\"paddingTop\":{\"value\":100,\"unit\":\"px\"},\"pageSize\":\"A4\",\"pageOrientation\":\"P\"} -->\n<div id=\"certificate\" class=\"certificate\">\n\t<div class=\"certificate-content-wrapper\">\n\t\t<!-- wp:spacer {\"height\":\"80px\"} -->\n\t\t<div style=\"height:80px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:columns -->\n\t\t<div class=\"wp-block-columns\">\n\t\t\t<!-- wp:column {\"width\":\"25%\"} -->\n\t\t\t<div class=\"wp-block-column\" style=\"flex-basis:25%\"></div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"verticalAlignment\":\"center\",\"width\":\"50%\"} -->\n\t\t\t<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:50%\">\n\t\t\t\t<!-- wp:image {\"align\":\"center\",\"id\":{{masteriyo_logo_image_id}},\"width\":158,\"height\":30,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n\t\t\t\t<figure class=\"wp-block-image aligncenter size-full is-resized\"><img src=\"{{masteriyo_logo_image_url}}\" alt=\"\" class=\"wp-image-{{masteriyo_logo_image_id}}\" width=\"158\" height=\"30\"/></figure>\n\t\t\t\t<!-- /wp:image -->\n\t\t\t</div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"width\":\"25%\"} -->\n\t\t\t<div class=\"wp-block-column\" style=\"flex-basis:25%\"></div>\n\t\t\t<!-- /wp:column -->\n\t\t</div>\n\t\t<!-- /wp:columns -->\n\n\t\t<!-- wp:spacer {\"height\":\"24px\"} -->\n\t\t<div style=\"height:24px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"typography\":{\"fontSize\":\"60px\",\"letterSpacing\":\"-0.4%\",\"fontStyle\":\"normal\",\"fontWeight\":\"500\",\"textTransform\":\"uppercase\"},\"color\":{\"text\":\"#272727\"}}} -->\n\t\t<p class=\"has-text-align-center has-text-color\"\n\t\t\tstyle=\"color:#272727;font-size:60px;font-style:normal;font-weight:500;text-transform:uppercase;letter-spacing:-0.4%\">\n\t\t\tCertificate</p>\n\t\t<!-- /wp:paragraph -->\n\n\t\t<!-- wp:spacer {\"height\":\"4px\"} -->\n\t\t<div style=\"height:4px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:columns {\"verticalAlignment\":\"center\"} -->\n\t\t<div class=\"wp-block-columns are-vertically-aligned-center\">\n\t\t\t<!-- wp:column {\"verticalAlignment\":\"center\",\"width\":\"25%\"} -->\n\t\t\t<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:25%\"></div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"verticalAlignment\":\"center\",\"width\":\"50%\"} -->\n\t\t\t<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:50%\">\n\t\t\t\t<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"typography\":{\"textTransform\":\"uppercase\",\"letterSpacing\":\"5px\"},\"color\":{\"text\":\"#272727\"}},\"fontSize\":\"medium\"} -->\n\t\t\t\t<p class=\"has-text-align-center has-text-color has-medium-font-size\"\n\t\t\t\t\tstyle=\"color:#272727;text-transform:uppercase;letter-spacing:5px\">of achievement</p>\n\t\t\t\t<!-- /wp:paragraph -->\n\t\t\t</div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"verticalAlignment\":\"center\",\"width\":\"25%\"} -->\n\t\t\t<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:25%\"></div>\n\t\t\t<!-- /wp:column -->\n\t\t</div>\n\t\t<!-- /wp:columns -->\n\n\t\t<!-- wp:spacer {\"height\":\"32px\"} -->\n\t\t<div style=\"height:32px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"color\":{\"text\":\"#606060\"}}} -->\n\t\t<p class=\"has-text-align-center has-text-color\" style=\"color:#606060\">This is to certify that</p>\n\t\t<!-- /wp:paragraph -->\n\n\t\t<!-- wp:spacer {\"height\":\"32px\"} -->\n\t\t<div style=\"height:32px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:masteriyo/student-name {\"clientId\":\"48fd1b40\",\"blockCSS\":\".masteriyo-student-name-block\\u002d\\u002d48fd1b40 { text-align: center; }\\n.masteriyo-student-name-block\\u002d\\u002d48fd1b40 { font-size: 42px; }\\n.masteriyo-student-name-block\\u002d\\u002d48fd1b40 { color: #272727; }\",\"alignment\":\"center\",\"fontSize\":{\"value\":42,\"unit\":\"px\"},\"textColor\":\"#272727\"} -->\n\t\t<div class=\"wp-block-masteriyo-student-name masteriyo-student-name-block--48fd1b40\">{{masteriyo_student_name}}\n\t\t</div>\n\t\t<!-- /wp:masteriyo/student-name -->\n\n\t\t<!-- wp:spacer {\"height\":\"40px\"} -->\n\t\t<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"color\":{\"text\":\"#606060\"}}} -->\n\t\t<p class=\"has-text-align-center has-text-color\" style=\"color:#606060\">has successfully completed and passed the\n\t\t\tcourse</p>\n\t\t<!-- /wp:paragraph -->\n\n\t\t<!-- wp:spacer {\"height\":\"40px\"} -->\n\t\t<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:masteriyo/course-title {\"clientId\":\"b6454285\",\"blockCSS\":\".masteriyo-course-title-block\\u002d\\u002db6454285 { text-align: center; }\\n.masteriyo-course-title-block\\u002d\\u002db6454285 { font-size: 24px; }\\n.masteriyo-course-title-block\\u002d\\u002db6454285 { color: #272727; }\",\"alignment\":\"center\",\"fontSize\":{\"value\":24,\"unit\":\"px\"},\"textColor\":\"#272727\"} -->\n\t\t<div class=\"wp-block-masteriyo-course-title masteriyo-course-title-block--b6454285\">{{masteriyo_course_title}}\n\t\t</div>\n\t\t<!-- /wp:masteriyo/course-title -->\n\n\t\t<!-- wp:spacer {\"height\":\"285px\"} -->\n\t\t<div style=\"height:285px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t<!-- /wp:spacer -->\n\n\t\t<!-- wp:columns -->\n\t\t<div class=\"wp-block-columns\">\n\t\t\t<!-- wp:column {\"width\":\"35%\"} -->\n\t\t\t<div class=\"wp-block-column\" style=\"flex-basis:35%\">\n\t\t\t\t<!-- wp:separator {\"style\":{\"color\":{\"background\":\"#434343\"}},\"className\":\"is-style-wide\"} -->\n\t\t\t\t<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\"\n\t\t\t\t\tstyle=\"background-color:#434343;color:#434343\" />\n\t\t\t\t<!-- /wp:separator -->\n\t\t\t</div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"width\":\"30%\"} -->\n\t\t\t<div class=\"wp-block-column\" style=\"flex-basis:30%\"></div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"width\":\"35%\"} -->\n\t\t\t<div class=\"wp-block-column\" style=\"flex-basis:35%\">\n\t\t\t\t<!-- wp:separator {\"style\":{\"color\":{\"background\":\"#434343\"}},\"className\":\"is-style-wide\"} -->\n\t\t\t\t<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\"\n\t\t\t\t\tstyle=\"background-color:#434343;color:#434343\" />\n\t\t\t\t<!-- /wp:separator -->\n\t\t\t</div>\n\t\t\t<!-- /wp:column -->\n\t\t</div>\n\t\t<!-- /wp:columns -->\n\n\t\t<!-- wp:columns {\"verticalAlignment\":\"center\"} -->\n\t\t<div class=\"wp-block-columns are-vertically-aligned-center\">\n\t\t\t<!-- wp:column {\"verticalAlignment\":\"center\",\"width\":\"35%\"} -->\n\t\t\t<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:35%\">\n\t\t\t\t<!-- wp:spacer {\"height\":\"8px\"} -->\n\t\t\t\t<div style=\"height:8px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n\t\t\t\t<!-- /wp:spacer -->\n\n\t\t\t\t<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"color\":{\"text\":\"#272727\"}}} -->\n\t\t\t\t<p class=\"has-text-align-center has-text-color\" style=\"color:#272727\">Date</p>\n\t\t\t\t<!-- /wp:paragraph -->\n\t\t\t</div>\n\t\t\t<!-- /wp:column -->\n\n\t\t\t<!-- wp:column {\"