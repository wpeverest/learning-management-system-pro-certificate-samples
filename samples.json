    "html": "<!-- wp:masteriyo/certificate {\"blockCSS\":\"\",\"backgroundImageURL\":\"{{masteriyo_background_image_url}}\",\"backgroundImageID\":{{masteriyo_background_image_id}},\"containerWidth\":{\"value\":85,\"unit\":\"%\"},\"paddingTop\":{\"value\":90,\"unit\":\"px\"},\"pageSize\":\"Letter\",\"pageOrientation\":\"L\"} -->\n<div id=\"certificate\" class=\"certificate\"><div class=\"certificate-content-wrapper\"><!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"large\"} -->\n<p class=\"has-text-align-center has-large-font-size\"><strong>CERTIFICATE OF ACHIEVEMENT</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"40px\"} -->\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"medium\"} -->\n<p class=\"has-text-align-center has-medium-font-size\">This is to certify that</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"40px\"} -->\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:masteriyo/student-name {\"clientId\":\"bea2edb3\",\"blockCSS\":\"\",\"alignment\":\"center\",\"fontSize\":{\"value\":40,\"unit\":\"px\"},\"textColor\":\"#000000\"} -->\n<div class=\"wp-block-masteriyo-student-name masteriyo-student-name-block--bea2edb3\">{{masteriyo_student_name}}</div>\n<!-- /wp:masteriyo/student-name -->\n\n<!-- wp:spacer {\"height\":\"40px\"} -->\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"medium\"} -->\n<p class=\"has-text-align-center has-medium-font-size\">has successfully completed and passed the course</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"40px\"} -->\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:masteriyo/course-title {\"clientId\":\"5e96fc07\",\"blockCSS\":\"\",\"alignment\":\"center\",\"fontSize\":{\"value\":40,\"unit\":\"px\"},\"textColor\":\"#000000\"} -->\n<div class=\"wp-block-masteriyo-course-title masteriyo-course-title-block--5e96fc07\">{{masteriyo_course_title}}</div>\n<!-- /wp:masteriyo/course-title -->\n\n<!-- wp:spacer {\"height\":\"223px\"} -->\n<div style=\"height:223px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column {\"width\":\"66.66%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:66.66%\"></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":\"33.33%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:33.33%\"><!-- wp:paragraph {\"align\":\"right\",\"fontSize\":\"medium\"} -->\n<p class=\"has-text-align-right has-medium-font-size\"><strong>John Doe</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"14px\"} -->\n<div style=\"height:14px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"align\":\"right\",\"fontSize\":\"medium\"} -->\n<p class=\"has-text-align-right has-medium-font-size\">Instructor</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div></div>\n<!-- /wp:masteriyo/certificate -->",
